QPushButton,
QPushButton:!hover,
QPushButton:!pressed {
    background: url(:resource);
    background-repeat: none;
    border-style: flat;
    margin-left: -25px;
    margin-top: 0px;
}

QPushButton:hover {
    background: url(:resource);
    background-repeat: none;
    margin-left: -25px;
    margin-top: -52px;
}

QPushButton:pressed {
    background: url(:resource);
    background-repeat: none;
    margin-left: -37px;
    margin-top: -52px;
}
